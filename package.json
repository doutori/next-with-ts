{
    "name": "next-with-ts",
    "version": "0.1.0",
    "description": "next.js with typescript boiler plate",
    "repository": {
        "type": "git",
        "url": "git@github.com:uruha/next-with-ts.git"
    },
    "scripts": {
        "dev": "nodemon server/index.ts",
        "build": "next build",
        "start": "NODE_ENV=production ts-node -O '{\"module\":\"commonjs\"}' server/index.ts",
        "type:check": "tsc",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint:fmt": "eslint . --fix --ext .ts,.tsx",
        "precommit": "lint-staged",
        "test": "jest"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint . --ext .ts,.tsx",
            "git add"
        ]
    },
    "keywords": [
        "next.js",
        "typescript"
    ],
    "author": "Kotaro Chiba",
    "license": "MIT",
    "dependencies": {
        "@zeit/next-typescript": "^1.1.1",
        "bunyan": "^1.8.12",
        "koa": "^2.7.0",
        "koa-router": "^7.4.0",
        "module-alias": "^2.2.0",
        "next": "^8.1.0",
        "next-redux-wrapper": "^3.0.0-alpha.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.3",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6",
        "ts-node": "^8.1.0",
        "typescript": "^3.4.5"
    },
    "devDependencies": {
        "@types/bunyan": "^1.8.6",
        "@types/enzyme": "^3.9.1",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.11",
        "@types/koa": "^2.0.48",
        "@types/koa-router": "^7.0.40",
        "@types/next": "^8.0.4",
        "@types/next-redux-wrapper": "^2.0.2",
        "@types/react": "^16.8.14",
        "@types/react-dom": "^16.8.4",
        "@types/react-redux": "^7.0.9",
        "@types/redux-logger": "^3.0.7",
        "@typescript-eslint/eslint-plugin": "^1.7.0",
        "@typescript-eslint/parser": "^1.7.0",
        "@typescript-eslint/typescript-estree": "^1.7.0",
        "babel-jest": "^24.7.1",
        "babel-plugin-module-resolver": "^3.2.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.12.1",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^2.3.0",
        "jest": "^24.7.1",
        "lint-staged": "^8.1.5",
        "nodemon": "^1.18.11",
        "prettier": "^1.17.0",
        "snapshot-diff": "^0.5.1",
        "ts-jest": "^24.0.2"
    },
    "_moduleAliases": {
        "~": "src"
    }
}
